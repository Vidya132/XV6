+int 
+getNumProc(void)
+{
+	int used = 0;
+	struct proc *p;
+
+  acquire(&ptable.lock);
+
+  for(p = ptable.proc; p < &ptable.proc[NPROC]; p++)
+    if(p->state != UNUSED)
+      used++;
+
+  release(&ptable.lock);
+	return used;
