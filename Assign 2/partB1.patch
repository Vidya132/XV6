+getMaxPid(void)
+{
+	int maxPid = 0;
+	struct proc *p;
+
+  acquire(&ptable.lock);
+
+  for(p = ptable.proc; p < &ptable.proc[NPROC]; p++)
+    if(p->state != UNUSED)
+      maxPid = (maxPid < p->pid) ? p->pid : maxPid;
+
+  release(&ptable.lock);
+	return maxPid;
+}
