+getProcInfo(int pid, struct processInfo* pi)
+{
+  struct proc *p = 0;
+  int found = 0;
+  acquire(&ptable.lock);
+  for(p = ptable.proc; p < &ptable.proc[NPROC]; p++){
+    if(p->state != UNUSED && p->pid == pid){
+        pi->ppid = p->parent->pid;
+        pi->psize = p->sz;
+        pi->numberContextSwitches = p->numcs;
+        found = 1;
+        break;
+      }
+  }
+  release(&ptable.lock);
+  if(found) return 0;
+  return -1;
+}
